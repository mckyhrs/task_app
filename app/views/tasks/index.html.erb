<h1>タスク一覧</h1>

<p><%= flash[:notice] %></p>

<table border=1>
	<thead>
		<tr>
			<th>ID</th>
			<th>タスク</th>
		</tr>
	</thead>

<% @tasks.each do |task| %>
	<tr>
		<td><%= task.id %></td>
		<td><%= link_to task.task_name, task_path(task) %></td>
	</tr>
<% end %>
</table>
<br/>
<%= link_to "新規タスク作成", new_task_path %>
